      <button type="button" class="btn btn-default csvcolumn" data-name="Gamme" data-key="5" data-sortable data-searchable>Gamme</button>
      <button type="button" class="btn btn-default csvcolumn" data-name="Prix" data-key="17" data-sortable data-filterable data-searchable>
        Prix
