<link class="csvinit" rel="stylesheet" data-attrs="href=tmplrelpath('../default.css')" data-init="ready_after_load()" />
<style>
#csvtable .csvcolumn {
  display: none;
}
#csvtable .csvcolumn[data-sortable] {
  display: block;
  cursor: pointer;
}
.items-list dl {
  margin-bottom: 2px;
}
.item-icons .item-icon {
  width: 30px;
  height: 30px;
}
.item-thumbnail {
  height: 100px;
}
.item-thumbnail img {
  clip: rect(0px, 110px, 100px, 0px);
}
</style>
<div data-init-block="row_icons">
	<!-- icon for men -->
  <img class="item-icon" data-init="if(not(eq(row.6,'1')),this.remove())" data-attrs="src=tmplrelpath('Homme.png')" />
  <!-- icon for women -->
  <img class="item-icon" data-init="if(not(eq(row.6,'2')),this.remove())" data-attrs="src=tmplrelpath('Femme.png')" />
  <!-- icon -->
  <img class="item-icon" data-attrs="src=tmplrelpath(row.9)" />
  <!-- wide icon -->
  <img class="item-icon" data-init="if(not(eq(row.3,'1')),this.remove())" data-attrs="src=tmplrelpath('Wide.png')" />
  <!-- top of the pop icon -->
  <img class="item-icon" data-init="if(not(eq(row.20,'1')),this.remove())" data-attrs="src=tmplrelpath('TopOfThePop.png')" />
</div>
