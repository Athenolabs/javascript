  <ul class="items-list">
    <li data-foreach="rows as i=>row" data-bind="click=open_row_page('#detailpage',i,row)">
      <!-- thumbnail -->
      <span class="item-thumbnail">
        <img data-attrs="src=row.10" />
      </span>
