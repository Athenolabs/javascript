---
title: Magazines
javascripts:
- lib/querystring.min.js
- lib/path.min.js
- lib/jquery.plist.js
- js/magazine.js
---
<div class="reader-container dark-bkg">
  <img class="reader-background" />
  <h4 class="headline text-center">{{title}}</h4>
  <div class="reader-content">
    <ul class="magazines">
      <li class="mag-item" data-id="default">
        <div class="mag-title" data-key="Title"></div>
        <div class="mag-subtitle" data-key="Subtitle"></div>
        <img class="mag-thumbnail" data-attr-keys="src=ThumbnailUrl" />
        <a href="#" class="label label-default mag-read-btn" data-key="magazines.read_btn"></a>
        <a href="#" class="label label-default mag-sample-btn mag-paid-type" data-key="magazines.sample_btn"></a>
      </li>
    </ul>
  </div>
</div>
