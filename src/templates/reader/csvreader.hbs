---
title: CSV Reader
javascripts:
- lib/querystring.min.js
- lib/path.min.js
- lib/async.js
- lib/d3.min.js
- lib/jquery.plist.js
- lib/taffy-min.js
- js/csvreader.js
stylesheets:
theme: light-bkg
---
<div id="csvfilters" role="form">
  <fieldset data-id="Range">
    <label data-content="name"></label>
    <div class="form-group">
      <label>From</label>
      <input type="number" class="form-control" placeholder="From" name="From" data-attrs="value=From" />
    </div>
    <div class="form-group">
      <label>To</label>
      <input type="number" class="form-control" placeholder="To" name="To" data-attrs="value=To" />
    </div>
  </fieldset>
  <fieldset data-id="MultipleChoice">
    <label data-content="name"></label>
    <select multiple class="form-control">
      <option data-foreach="options as option" data-attrs="value=option;selected=get(checked_options,option)" data-content="option"></option>
    </select>
  </fieldset>
</div>
<div id="csvreader">
  <table id="csvtable">
    <tbody>
      <tr data-foreach="rows as row">
        <td data-foreach="row as col" data-content="col"></td>
      </tr>
    </tbody>
  </table>
</div>
