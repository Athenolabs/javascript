<div class="row" id="setup-wrapper" data-setup-file-path="APP_/Uploads/setup-html5.plist" data-upload-dir="APP_/Uploads/">
  <div class="col-lg-12">
    <div class="box">
      <header>
        <h5 localize>StyleSheets</h5>
      </header>
      <div id="collapse4" class="body">
        <form class="form-horizontal">

          <!-- stylesheet -->
          <div class="form-group">
            <label class="control-label col-lg-4" localize>CSS For Covers Module</label>
            <div class="col-lg-8">
              <div class="fileinput fileinput-new" data-provides="fileinput">
				        <div>
				          <span class="btn btn-default btn-file"><span class="fileinput-new" localize>Select css file</span><span class="fileinput-exists fileinput-change" localize>Change</span><input data-type="file" type="file" name="style_covers.css" accept="text/css"></span>
				          <a href="#" class="btn btn-default fileinput-exists fileinput-remove" data-dismiss="fileinput" localize>Remove</a>
				        </div>
			        </div>
            </div>
          </div>

          <!-- stylesheet -->
          <div class="form-group">
            <label class="control-label col-lg-4" localize>CSS For PDFReader</label>
            <div class="col-lg-8">
              <div class="fileinput fileinput-new" data-provides="fileinput">
				        <div>
				          <span class="btn btn-default btn-file"><span class="fileinput-new" localize>Select css file</span><span class="fileinput-exists fileinput-change" localize>Change</span><input data-type="file" type="file" name="style_pdfreader.css" accept="text/css"></span>
				          <a href="#" class="btn btn-default fileinput-exists fileinput-remove" data-dismiss="fileinput" localize>Remove</a>
				        </div>
			        </div>
            </div>
          </div>
            
        </form>
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <div class="box">
      <header>
        <h5 localize>Subscriptions</h5>
      </header>
      <div id="collapse4" class="body">
        <form class="form-horizontal">
          
          <!-- Subscription 1 title -->
          <div class="form-group">
            <label for="Subscription_1_Title" class="control-label col-lg-4" localize>Subscription 1 title</label>
            <div class="col-lg-6">
              <input type="text" name="Subscription_1_Title" id="Subscription_1_Title" class="form-control">
            </div>
		      </div>

          <!-- Subscription 1 link -->
          <div class="form-group">
            <label for="Subscription_1_Link" class="control-label col-lg-4" localize>Subscription 1 link</label>
            <div class="col-lg-6">
              <input type="text" name="Subscription_1_Link" id="Subscription_1_Link" class="form-control">
            </div>
		      </div>

          <!-- Subscription 2 title -->
          <div class="form-group">
            <label for="Subscription_2_Title" class="control-label col-lg-4" localize>Subscription 2 title</label>
            <div class="col-lg-6">
              <input type="text" name="Subscription_2_Title" id="Subscription_2_Title" class="form-control">
            </div>
		      </div>

          <!-- Subscription 2 link -->
          <div class="form-group">
            <label for="Subscription_2_Link" class="control-label col-lg-4" localize>Subscription 2 link</label>
            <div class="col-lg-6">
              <input type="text" name="Subscription_2_Link" id="Subscription_2_Link" class="form-control">
            </div>
		      </div>

        </form>
      </div>
    </div>
  </div>
</div>
